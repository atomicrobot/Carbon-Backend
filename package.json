{
  "name": "api",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "TZ=UTC node -r module-alias/register ./dist",
    "start:dev": "TZ=UTC nodemon --config nodemon.json",
    "test": "TZ=UTC nodemon --config nodemon.test.json",
    "test:nodebug": "TZ=UTC node -r ts-node/register -r tsconfig-paths/register ./spec"
  },
  "_moduleAliases": {
    "@models": "dist/models",
    "@daos": "dist/daos",
    "@clients": "dist/clients",
    "@services": "dist/services",
    "@util": "dist/util",
    "@server": "dist/server"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^3.0.6",
    "@google-cloud/secret-manager": "^2.1.0",
    "google-auth-library": "^5.10.1",
    "command-line-args": "^5.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-correlation-id": "^1.3.1",
    "express-openapi-validator": "^4.0.0-beta.2",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "response-time": "^2.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/express": "^4.17.8",
    "@types/express-correlation-id": "^1.2.2",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.5.14",
    "@types/jsonfile": "^6.0.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.2",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.10",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.1",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}