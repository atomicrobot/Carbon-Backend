{
  "name": "api",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist/*",
    "do-build": "node ./util/build.js",
    "build": "npm-run-all clean do-build",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "TZ=UTC node -r module-alias/register ./dist",
    "start:dev": "TZ=UTC nodemon --config nodemon.json",
    "openapi:start-and-save": "npm-run-all openapi:remove openapi:save",
    "openapi:start-save-and-kill": "npm-run-all openapi:remove openapi:save-and-kill",
    "openapi:remove": "rimraf openapi.json",
    "openapi:save": "concurrently \"npm run start:dev\" \"wait-on http://localhost:3000/api-docs.json && curl http://localhost:3000/api-docs.json > openapi.json\"",
    "openapi:save-and-kill": "concurrently \"npm run start:dev\" \"wait-on http://localhost:3000/api-docs.json && curl http://localhost:3000/api-docs.json > openapi.json\" \"wait-on openapi.json -s 1 && npm run stop\"",
    "test": "TZ=UTC nodemon --config nodemon.test.json",
    "test:nodebug": "TZ=UTC node -r ts-node/register -r tsconfig-paths/register ./spec",
    "stop": "pkill SIGINT node"
  },
  "_moduleAliases": {
    "@app": "dist/app",
    "@models": "dist/models",
    "@daos": "dist/daos",
    "@clients": "dist/clients",
    "@services": "dist/services",
    "@util": "dist/util"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.0.0",
    "@google-cloud/secret-manager": "^3.2.0",
    "command-line-args": "^5.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-correlation-id": "^1.3.1",
    "express-openapi-validator": "^4.8.0",
    "google-auth-library": "^6.1.0",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/express": "^4.17.8",
    "@types/express-correlation-id": "^1.2.2",
    "@types/find": "^0.2.1",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "^3.5.14",
    "@types/jsonfile": "^6.0.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.11",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.3",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.1"
  }
}
